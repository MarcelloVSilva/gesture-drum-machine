{
  "name": "gesture-drum-machine",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "build": "babel ./src --out-dir dist",
    "buildJs": "browserify ./public/javascripts/withPreModel.js -o dist/bundle.js",
    "watch": "nodemon"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^1.2.9",
    "@tensorflow/tfjs-node": "^1.2.9",
    "browserify": "^16.5.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "reading-time": "^1.2.0",
    "rimraf": "^3.0.0",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "nodemon": "^1.19.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
